{
  "name": "prestamos",
  "version": "0.0.1",
  "description": "App para prestamos",
  "main": "./dist/App.js",
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/**/*.html dist && copyfiles -u 1 src/**/*.css dist && copyfiles -u 1 src/database/database.sqlite dist",
